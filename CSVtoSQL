import sqlite3


def make_database():
    conn = sqlite3.connect('CSVtoSQL.db')
    c = conn.cursor()

    c.execute('''CREATE TABLE cases (
id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
ourId TEXT NOT NULL,
party TEXT NOT NULL,
counterParty TEXT,
description TEXT,
officeLocation TEXT,
caseNumbers TEXT,
vps TEXT,
status TEXT, 
tStamp TEXT DEFAULT CURRENT_TIMESTAMP)''')

    conn.commit()
